    <h2  class="poppins-semibold">Run</h2><hr>
      <p class="poppins-regular">Attach files to project<p>
    <form class="dropzone-box" method="post" enctype="multipart/form-data" action="http://localhost:3000/upload" >
      <div class="dropzone-area run">
        <p class="poppins-regular"> Click to upload</p>
        <input type="file" required id="upload-file"
        name="files" multiple>
        <% if(locals.file){ %>
          <p class="message poppins-regular"><%= file %> files Uploaded successfully
          </p>
        <% }else{ %>
          <p class="message upload poppins-regular">No Files Selected</p>
        <% } %> 
        <video autoplay controls src="" type="video/mp4" class="transparent" id="upload-file" width="520" height="440" hidden></video> 
      </div>
      <div class="dropzone-actions">
        <button type="reset" class="poppins-regular"> Cancel</button>
        <button type="submit" id="submit-button" class="poppins-regular">Save 
        </button>
      </div>
    </form>
  </section>

